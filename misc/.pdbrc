alias uu u;;ll
alias dd d;;ll
alias ijson __import__('json')
alias curl print("curl -X {method} -H {headers} -d '{data}' '{uri}'".format(method=%1.method, headers=" -H ".join(['"{0}: {1}"'.format(k, v) for k, v in %1.headers.items()]), data=%1.body, uri=%1.url))
alias copy __import__('pyperclip').copy(__import__('json').dumps(%1))
